nd for the current value that is currently map, i stand for index\r\n          array.map(function(currentValue, index, arr), thisValue) */}\r\n          {tokenArr.map((e, i) => {\r\n            return (\r\n              <div\r\n                className=\"tokenChoice\"\r\n                key={i}\r\n                onClick = {() => modifyToken(i)}  >\r\n\r\n                <img src={e.img} alt={e.ticker} className=\"tokenLogo\" />\r\n                <div className=\"tokenChoiceNames\">\r\n                  <div className=\"tokenName\">{e.name}</div>\r\n                  <div className=\"tokenTicker\">{e.ticker}</div>\r\n                </div>\r\n              </div>\r\n            );\r\n          })}\r\n        </div>\r\n      </Modal>\r\n\r\n      <div className=\"content\">\r\n        <div className=\"tradeBox\">\r\n          <div className=\"tradeBoxHeader\">\r\n            <h3>Swap</h3>\r\n            <Popover\r\n              title=\"Setting\"\r\n              content={settingContent}\r\n              trigger=\"click\"\r\n              placement=\"bottomRight\"\r\n            >\r\n              <SettingsIcon className=\"cog\" />\r\n            </Popover>\r\n          </div>\r\n          <div className=\"inputs\">\r\n            <Input\r\n              placeholder=\"0.00\"\r\n              value={tokenOneAmount}\r\n              onChange={changeTokenAmount}\r\n            />\r\n            <div className=\"asset assetOne\" onClick={() => openModal(1)}>\r\n              <img\r\n                src={tokenOne.img}\r\n                alt=\"assetOneLogo\"\r\n                className=\"assetLogo\"\r\n              />\r\n              {tokenOne.ticker}\r\n              <ArrowDownIcon />\r\n            </div>\r\n\r\n            <div className=\"switchButton\" onClick={SwitchTokens}>\r\n              <UpDownIcon className=\"switchArrow\" />\r\n            </div>\r\n\r\n            <Input placeholder=\"0.00\" value={tokenTwoAmount} disabled={true} />\r\n            <div className=\"asset assetTwo\" onClick={() => openModal(2)}>\r\n              <img\r\n                src={tokenTwo.img}\r\n                alt=\"assetTwoLogo\"\r\n                className=\"assetLogo\"\r\n              />\r\n              {tokenTwo.ticker}\r\n              <ArrowDownIcon />\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default Swap;\r\n",[],"C:\\Users\\Admin\\Documents\\Web\\Web3.0\\UniSwap-Clone-2\\dexStarter\\dex\\src\\components\\CarouselHead.js",[],[],"C:\\Users\\Admin\\Documents\\Web\\Web3.0\\UniSwap-Clone-2\\dexStarter\\dex\\src\\components\\Footer.js",["64","65","66","67","68"],[],{"ruleId":"69","severity":1,"message":"70","line":1,"column":17,"nodeType":"71","messageId":"72","endLine":1,"endColumn":25},{"ruleId":"69","severity":1,"message":"73","line":1,"column":27,"nodeType":"71","messageId":"72","endLine":1,"endColumn":36},{"ruleId":"69","severity":1,"message":"74","line":5,"column":8,"nodeType":"71","messageId":"72","endLine":5,"endColumn":17},{"ruleId":"69","severity":1,"message":"75","line":6,"column":8,"nodeType":"71","messageId":"72","endLine":6,"endColumn":18},{"ruleId":"76","severity":1,"message":"77","line":54,"column":13,"nodeType":"78","endLine":54,"endColumn":125},{"ruleId":"76","severity":1,"message":"77","line":55,"column":13,"nodeType":"78","endLine":55,"endColumn":126},{"ruleId":"76","severity":1,"message":"77","line":56,"column":13,"nodeType":"78","endLine":56,"endColumn":129},{"ruleId":"69","severity":1,"message":"70","line":3,"column":17,"nodeType":"71","messageId":"72","endLine":3,"endColumn":25},{"ruleId":"69","severity":1,"message":"73","line":3,"column":27,"nodeType":"71","messageId":"72","endLine":3,"endColumn":36},{"ruleId":"69","severity":1,"message":"79","line":5,"column":8,"nodeType":"71","messageId":"72","endLine":5,"endColumn":17},{"ruleId":"69","severity":1,"message":"80","line":7,"column":10,"nodeType":"71","messageId":"72","endLine":7,"endCol